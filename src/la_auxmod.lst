         72     2  !        UPPER CASE 'Z'.
         73     2  !
         74     2              IF( INTA.GE.97 .AND. INTA.LE.122 ) INTA = INTA - 32
         75     2              IF( INTB.GE.97 .AND. INTB.LE.122 ) INTB = INTB - 32
