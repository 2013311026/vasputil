         85     2           IF( INTB.GE.129 .AND. INTB.LE.137 .OR.                         &
         86     2       &       INTB.GE.145 .AND. INTB.LE.153 .OR.                         &
         87     2       &       INTB.GE.162 .AND. INTB.LE.169 ) INTB = INTB + 64
         88     2  !
         89     2           ELSE IF( ZCODE.EQ.218 .OR. ZCODE.EQ.250 )THEN
